{
  "name": "@nindroidsystems/ui",
  "version": "1.1.5",
  "description": "UI components for Nindroid Systems projects",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
