    "dev": "tsup --watch"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^24.6.1",
    "@types/react": "^19.1.16",
    "framer-motion": "^12.23.22",
    "react": "^19.1.1",
    "tailwindcss": "^4.1.13",
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "eslint": "^9.36.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "react-router-dom": "^7.9.3",
    "typescript-eslint": "^8.45.0"
  }
